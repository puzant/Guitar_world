    <div class="row text-center text-lg-left">
        <% for(let i=0; i<tabs.length; i++) { %>
            <div class="col-lg-3 col-md-4 col-xs-6 thumbnail">
                <!-- <a href="<%= tabs[i].image %>" class="d-block mb-4 h-100" data-fancybox="gallery"> -->
            <a href="<%= tabs[i].image %>" class="d-block mb-4 h-100">
          <img src="<%= tabs[i].image %>"  class="img-thumbnail">
          <div class="overlay">
            <div class="text">
              <div class="">Name: <%= tabs[i].name %></div>
              <div class="">Composer: <%= tabs[i].composer %></div>
              <div class="">Genre: <%= tabs[i].genre %></div>
              <hr>
              <div class="like">
              <button class="btn btn-danger like"><i class="fas fa-thumbs-up"></i></button>
              <button class="btn btn-danger dilike"><i class="fas fa-thumbs-down"></i></button>
              </div>
              </div>   
            </div>
          </a>
          </div>
          <% } %>
